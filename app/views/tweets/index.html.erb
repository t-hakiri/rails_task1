
<div class="top_manu">
<h1>Tweetクローン</h1>

<p><%= notice %></p>
<table>
	<tr>
		<th>Tweet一覧</th>
	</tr>

	<% @tweets.each do |tweet| %>
		<tr>
			<td><%= tweet.content %></td>
			
			<td>
			<%= link_to "詳細を確認する", tweet_path(tweet.id) %>
			</td>

			<td>
			<%= link_to "編集する", edit_tweet_path(tweet.id) %>
			</td> 
		</tr>
	<% end %>
	<%= link_to("トップページ", root_path) %>
</table>


</div>

